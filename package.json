{
  "name": "bitvavo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:stylelint": "stylelint --cache --fix **/*.css",
    "lint": "run-p lint:tsc lint:eslint lint:stylelint",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "lint-staged": {
    "*": "prettier -uw --cache",
    "*.css": "stylelint --cache --fix",
    "*.{ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.9",
    "@tanstack/react-table": "^8.11.3",
    "@tanstack/react-virtual": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
